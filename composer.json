{
	"name": "webdna/commerce-bundles",
	"description": "Bundles plugin for Craft Commerce",
	"type": "craft-plugin",
	"version": "2.0.0-beta.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"bundles"
	],
	"support": {
		"docs": "https://github.com/webdna/commerce-bundles/blob/master/README.md",
		"issues": "https://github.com/webdna/commerce-bundles/issues"
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "WebDNA",
			"homepage": "https://webdna.co.uk"
		}
	],
	"require": {
		"craftcms/cms": "^4.0",
		"craftcms/commerce": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"webdna\\commerce\\bundles\\": "src/"
		}
	},
	"extra": {
		"name": "Bundles",
		"handle": "commerce-bundles",
		"hasCpSettings": false,
		"hasCpSection": true,
		"changelogUrl": "https://raw.githubusercontent.com/webdna/commerce-bundles/master/CHANGELOG.md",
		"components": {
			"bundlesService": "webdna\\commerce\\bundles\\services\\BundlesService"
		},
		"class": "webdna\\commerce\\bundles\\Bundles"
	}
}
